{
    "seatown": {
        "title": "Seatown",
        "serverroot": "/mnt/thumb1",
        "captureroot": "/mnt/thumb1/capture",
        "subdir": "Seatown",
        "port": 8080,
        "pantilt": true,
        "schedule": [
            {
                "#": "nights are as interesting as days",
                "hours": ".*",
                "interval": 3
            }
         ]
    },

    "cannerycove": {
        "title": "CanneryCove",
        "serverroot": "/mnt/thumb1",
        "captureroot": "/mnt/thumb1/capture",
        "subdir": "CanneryCove",
        "port": 8080,
        "pantilt": false,
        "schedule": [
            {
                "#": "6am-9pm",
                "hours": "[6-9]|1[0-9]|2[0-1]",
                "interval": 1
            },
            {
                "#": "anything else",
                "hours": ".*",
                "interval": 20
            }
        ]
    },

    "cannerycoveIR": {
        "title": "CanneryCoveIR",
        "serverroot": "/mnt/thumb1",
        "captureroot": "/mnt/thumb1/capture",
        "subdir": "CanneryCoveIR",
        "port": 8080,
        "pantilt": false,
        "schedule": [
            {
                "#": "8-11pm, 12-6am",
                "hours": "2[0-3]|[0-6]",
                "interval": 1
            },
            {
                "#": "7am-7pm",
                "hours": "[7-9]|1[0-9]",
                "interval": 20
            }
        ]
    }
}
